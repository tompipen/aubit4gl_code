database formonly

screen
{
 Carta:[f000 |f001                                    ]
 Stampante:[f002 |f003                                    ]

 Inizializzazione:[f004                                    ]

}
end

attributes

f000 = formonly.link_frm type smallint not null, autonext, reverse,
       include = (1 to 32000),
       comments = "Immettere il codice richiesto o Ctrl-b per aiuto";
f001 = formonly.desc_frm, noentry;
f002 = formonly.link_ptr type smallint not null, autonext, reverse,
       include = (1 to 32000),
       comments = "Immettere il codice richiesto o Ctrl-b per aiuto";
f003 = formonly.desc_ptr, noentry;
f004 = formonly.init, autonext, reverse;

instructions

delimiters "  ";

end
