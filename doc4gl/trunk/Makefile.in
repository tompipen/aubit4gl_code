include FgldocDefs.mk

.PHONY:all
all:
	$(MAKE) -C parser all
	$(MAKE) -C manager all
	$(MAKE) -C browser all

.PHONY:install
install:
	$(MAKE) -C parser install
	$(MAKE) -C manager install
	$(MAKE) -C browser install

.PHONY:clean
clean:
	$(MAKE) -C parser clean
	$(MAKE) -C manager clean
	$(MAKE) -C browser clean

