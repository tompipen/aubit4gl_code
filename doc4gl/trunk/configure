   { (exit 1); exit 1; }; }
	    	fi
		else
			{ { echo "$as_me:4942: error: Cannot read Perl version." >&5
echo "$as_me: error: Cannot read Perl version." >&2;}
   { (exit 1); exit 1; }; }
		fi
		#is is a link?
		if test -L "/usr/bin/perl"; then
			#it's a link, maybe to the same perl executable?
			#lrwxrwxrwx    1 root     root           12 Dec 23  2001 X11 -> ../X11R6/bin
			echo "$as_me:4950: result: /usr/bin/perl is link to $PERL (OK)" >&5
echo "${ECHO_T}/usr/bin/perl is link to $PERL (OK)" >&6
        else
            #not a link, this is another perl executable there.
            #maybe it's the same version?
			echo "$as_me:4955: result: /usr/bin/perl is same version executable" >&5
echo "${ECHO_T}/usr/bin/perl is same version executable" >&6
		fi
    else
        #we should create a link
        ln $PERL /usr/bin/perl
    fi
else
	echo "$as_me:4963: result: OK" >&5
