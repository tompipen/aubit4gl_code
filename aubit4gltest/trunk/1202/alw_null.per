DATABASE formonly 
SCREEN
{
        
    smint: [a       ] 
  decimal: [b       ]
   string: [c       ]

}

	
ATTRIBUTES
a = formonly.a type smallint, include = (NULL, 1 TO 999999);
b = formonly.b type decimal, include = (NULL, 0.01 TO 999.99);
c = formonly.c type char, include = (NULL, 'YES', 'NO');
END

INSTRUCTIONS
DELIMITERS " "
