/*
=====================================================================
                        Source: close.rule
=====================================================================
*/


close_cmd	:	CLOSE_FORM form_name { print_close('F',$<str>2); add_feature("CLOSE_FORM");}
		|	CLOSE_WINDOW win_name { print_close('W',$<str>2); add_feature("CLOSE_WINDOW");}
		|	CLOSE_STATUSBOX win_name { print_close('W',$<str>2); }
		|	CLOSE_DATABASE { add_feature("CLOSE_DATABASE"); print_close('D',0); add_feature("CLOSE_DATABASE");}
		|	CLOSE_SESSION conn_id { add_feature("CLOSE_SESSION"); print_close('S',$<str>2); add_feature("CLOSE_SESSION");}
		|	CLOSE fetch_cursor_name { add_feature("CLOSE_CURSOR"); print_close('C',$<str>2); add_feature("CLOSE_CURSOR");}
;
