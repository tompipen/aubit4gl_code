*/

code_cmd : KW_CSTART
emb_code 
KW_CEND
;

emb_code : sql_code | c_code;
sql_code : sql_code_part | sql_code sql_code_part
;

sql_code_part: SQLLINE {
			lex_printc("%s",$<str>1);
}
;

c_code : c_code_part | c_code c_code_part

;

c_code_part  : CLINE {
			lex_printc("%s\n",$<str>1);
}
;
