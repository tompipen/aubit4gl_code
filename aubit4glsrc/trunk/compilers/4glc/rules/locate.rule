locate_cmd : LOCATE variable IN_MEMORY {
int vtype;
vtype=scan_variable($<str>2);
if (vtype!=11&&vtype!=12) {
a4gl_yyerror("Only Byte & Text may be located...");
YYERROR;
}
print_locate('M',$<str>2,"0");


}
| LOCATE variable IN_FILE file_name {
int vtype;
vtype=scan_variable($<str>2);
if (vtype!=11&&vtype!=12) {
a4gl_yyerror("Only Byte & Text may be located...");
YYERROR;
}

print_locate('F',$<str>2,$<str>4);
}
;


