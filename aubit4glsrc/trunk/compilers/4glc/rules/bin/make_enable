#!/bin/sh
awk '
{
kw=$1
if (def[kw]) next
def[kw]=1
kwl=tolower(kw)
print  kw " : KWS_" kw ";"
}

END {
printf("\n /* ================ from make_enable ================= */\n\n");

printf("NAMED : NAMED_GEN\n");
for (a in def) {
	printf(" | KWS_%s\n",a);
}
printf(";\n")
printf("\n /* ================ end make_enable ================= */\n\n");
}
' < generated/single_words > generated/single_words.rule




