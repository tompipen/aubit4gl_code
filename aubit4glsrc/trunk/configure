#andrej@istation-vm-xp /usr/local/aubit4gl/tools/test
#$ bash --version
#GNU bash, version 3.1.17(6)-release (i686-pc-cygwin)
#Copyright (C) 2005 Free Software Foundation, Inc.
#
#$ echo "aaa`bin/aubit-config TARGET_OS`xxx"
#aaamingwxxx

