       # Append any errors to the config.log.
       cat out/conftest.err 1>&5
     else
       lt_cv_compiler_c_o=yes
     fi
   fi
   CFLAGS="$save_CFLAGS"

   chmod u+w .
   $rm conftest* out/*
   rmdir out
   cd ..
   rmdir conftest
   $rm conftest*

fi
