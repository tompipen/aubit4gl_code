    shlibpath_overrides_runpath=yes
#    allow_undefined_flag=
#    always_export_symbols=yes

#      archive_cmds='$CC -shared $libobjs $deplibs $compiler_flags ${wl}-soname $wl$soname
#-o $lib ${wl}--image-base=0x10000000'

