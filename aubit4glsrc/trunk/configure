		if test "$PG_ESQLC_VERSION2" != ""; then
			case "$PG_ESQLC_VERSION2" in

				*informix*)
			    	{ $as_echo "$as_me:$LINENO: result: Yes-$PG_ESQLC_VERSION $PG_ESQLC_VERSION2" >&5
$as_echo "Yes-$PG_ESQLC_VERSION $PG_ESQLC_VERSION2" >&6; }
					PG_ESQLC_IFXCOMPAT=yes
		            ;;
