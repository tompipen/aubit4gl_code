	A4GL_PACKER=XML
	A4GL_PACKER_DETECTED="$A4GL_PACKER_DETECTED $A4GL_PACKER"

   	if test "$HAVE_RPCLIB" != "0"; then
		A4GL_PACKER=XDR
		A4GL_PACKER_DETECTED="$A4GL_PACKER_DETECTED $A4GL_PACKER"
    fi

	#A4GL_PACKER=PERL

	#Why would we use anything else anyway?
	#Simple data
	A4GL_PACKER=PACKED
	A4GL_PACKER_DETECTED="$A4GL_PACKER_DETECTED $A4GL_PACKER"

