	#FIXME: do we want to use XDR forms any more?
	if test "$HAVE_RPCLIB" != "0"; then
		A4GL_FORMTYPE="XDR"
		A4GL_FORMTYPE_DETECTED="$A4GL_FORMTYPE_DETECTED $A4GL_FORMTYPE"
    fi

	A4GL_FORMTYPE="GENERIC"
	A4GL_FORMTYPE_DETECTED="$A4GL_FORMTYPE_DETECTED $A4GL_FORMTYPE"
