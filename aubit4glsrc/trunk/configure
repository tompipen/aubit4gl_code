$ac_datarootdir_hack
" $ac_file_inputs | sed -f "$tmp/subs-1.sed" | sed -f "$tmp/subs-2.sed" | sed -f "$tmp/subs-3.sed" | sed -f "$tmp/subs-4.sed" | sed -f "$tmp/subs-5.sed" | sed -f "$tmp/subs-6.sed" | sed -f "$tmp/subs-7.sed" | sed -f "$tmp/subs-8.sed" | sed -f "$tmp/subs-9.sed" | sed -f "$tmp/subs-10.sed" | sed -f "$tmp/subs-11.sed" | sed -f "$tmp/subs-12.sed" | sed -f "$tmp/subs-13.sed" | sed -f "$tmp/subs-14.sed" >$tmp/out

test -z "$ac_datarootdir_hack$ac_datarootdir_seen" &&
  { ac_out=`sed -n '/\${datarootdir}/p' "$tmp/out"`; test -n "$ac_out"; } &&
  { ac_out=`sed -n '/^[	 ]*datarootdir[	 ]*:*=/p' "$tmp/out"`; test -z "$ac_out"; } &&
