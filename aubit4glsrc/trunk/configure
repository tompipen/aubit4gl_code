echo "$as_me: WARNING: V8 on Windows - probably not" >&2;}
						PG_ENGINE_IFXCOMPAT=yes
					else
						PG_ENGINE_IFXCOMPAT=maybe
					fi
		            ;;
				*)
			    	{ echo "$as_me:$LINENO: WARNING: Invalid version number format-$POSTMASTER_VER" >&5
echo "$as_me: WARNING: Invalid version number format-$POSTMASTER_VER" >&2;}
					PG_ENGINE_IFXCOMPAT=unknown
				    ;;
	        esac

			{ echo "$as_me:$LINENO: checking PG engine IFX compatibility-informix_serial function" >&5
echo $ECHO_N "checking PG engine IFX compatibility-informix_serial function... $ECHO_C" >&6; }
			TMPooo=`nm -A --defined-only $POSTMASTER 2>&1`
			if test "$TMPooo" != ""; then
				case "$TMPooo" in
				*no*symbols)
					{ echo "$as_me:$LINENO: result: unknown" >&5
echo "${ECHO_T}unknown" >&6; }
					{ echo "$as_me:$LINENO: WARNING: ** No symbols in PG executable" >&5
echo "$as_me: WARNING: ** No symbols in PG executable" >&2;}
					;;
				*informix_serial*)
					{ echo "$as_me:$LINENO: result: yes" >&5
echo "${ECHO_T}yes" >&6; }
					#Note: how compatible is it, thats the question
					PG_ENGINE_IFXCOMPAT=yes
					;;
				*)
					{ echo "$as_me:$LINENO: result: unknown-no result from nm" >&5
echo "${ECHO_T}unknown-no result from nm" >&6; }
					;;
				esac
			fi

			{ echo "$as_me:$LINENO: checking PG engine IFX compatibility" >&5
echo $ECHO_N "checking PG engine IFX compatibility... $ECHO_C" >&6; }
			{ echo "$as_me:$LINENO: result: $PG_ENGINE_IFXCOMPAT" >&5
echo "${ECHO_T}$PG_ENGINE_IFXCOMPAT" >&6; }
        else
			{ echo "$as_me:$LINENO: WARNING: No locally installed postmaster detected" >&5
echo "$as_me: WARNING: No locally installed postmaster detected" >&2;}
		fi

	CFLAGS="$old_CFLAGS"; LDFLAGS="$old_LDFLAGS"; LIBS="$old_LIBS"; CPPFLAGS="$old_CPPFLAGS"
