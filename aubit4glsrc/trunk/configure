       # Append any errors to the config.log.
       cat conftest.err 1>&5
     else
       lt_cv_prog_cc_pic_works=yes
     fi
   fi
   $rm conftest*
   CFLAGS="$save_CFLAGS"

fi
echo "$as_me:5263: result: $lt_cv_prog_cc_pic_works" >&5
echo "${ECHO_T}$lt_cv_prog_cc_pic_works" >&6

if test x"$lt_cv_prog_cc_pic_works" = xyes; then
