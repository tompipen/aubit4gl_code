			RET=$?
			if test "$RET" = "0"; then
				PCRE_INST_PATH=`regtool get "$PCRE_REG_PATH/InstallPath"`
				#echo "PCRE_INST_PATH=$PCRE_INST_PATH"
				PCRE_INST_PATH=`$CYGPATH -ws "$PCRE_INST_PATH"`
				#echo "PCRE_INST_PATH=$PCRE_INST_PATH"
