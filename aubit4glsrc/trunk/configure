	LIBLOGICAL_BUILD="LOGREPPROC_TXT LOGREPPROC_CSV LOGREPPROC_HTML"
	LIBLOGICAL_GTK2_DEPEND="LOGREP_CSV LOGREP_TXT LOGREP_HTML"
	LIBLOGICAL_PDF_DEPEND="LOGREPPROC_PDF"
	LIBLOGICAL_PDF_GTK2_DEPEND="LOGREP_PDF"
