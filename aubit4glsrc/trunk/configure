       cat conftest.err 1>&5
     else
       lt_cv_prog_cc_rtti_exceptions=yes
     fi
   fi
   $rm conftest*
   CFLAGS="$save_CFLAGS"

fi
echo "$as_me:$LINENO: result: $lt_cv_prog_cc_rtti_exceptions" >&5
