	#this creates full set dump on Solaris
	set `$MAKE --version | head -n 1 | sed -e 's/^GNU Make version //' -e 's/^GNU Make //' -e 's/, by Richard Stallman and Roland McGrath.//' -e 's/\./ /g'`
	if test "$1" != "" -a "$2" != ""; then
		if test "$1" = "3" -a "$2" -lt "50"; then
	        { echo "$as_me:$LINENO: WARNING: You will need GNU make version 3.50 or later (found $1.$2.$3)." >&5
echo "$as_me: WARNING: You will need GNU make version 3.50 or later (found $1.$2.$3)." >&2;}
        else
