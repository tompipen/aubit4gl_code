  case $ac_dest in
    default-1 ) test -z "$CONFIG_HEADERS" || echo timestamp > incl/stamp-h ;;
  esac
done
EOF

cat >>$CONFIG_STATUS <<\EOF

{ (exit 0); exit 0; }
