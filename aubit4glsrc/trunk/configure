			PCRE_INC_PATH="`$CYGPATH -m $withval`"
			CPPFLAGS="-I$PCRE_INC_PATH/include $CPPFLAGS"
			PCRE_LIB_PATH="`$CYGPATH -m $withval`"
			LDFLAGS="-L$PCRE_LIB_PATH/bin $LDFLAGS"
