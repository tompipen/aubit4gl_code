$as_echo "allow it anyway - set HASV3" >&6; }
							;;
							*) 	PG_ESQLC_IFXCOMPAT=no
								{ $as_echo "$as_me:$LINENO: result: not allowed" >&5
$as_echo "not allowed" >&6; }
							;;
						esac
					else
				    		{ $as_echo "$as_me:$LINENO: result: No-'informix' identifier not found-$PG_ESQLC_VERSION $PG_ESQLC_VERSION2" >&5
$as_echo "No-'informix' identifier not found-$PG_ESQLC_VERSION $PG_ESQLC_VERSION2" >&6; }
						{ $as_echo "$as_me:$LINENO: result: recent ECPG so will allow it anyway" >&5
$as_echo "recent ECPG so will allow it anyway" >&6; }
						PG_ESQLC_IFXCOMPAT=yes
					fi
				    ;;
	        esac
        else
	    	{ $as_echo "$as_me:$LINENO: result: No-Cannot read version number" >&5
$as_echo "No-Cannot read version number" >&6; }
			PG_ESQLC_IFXCOMPAT=no
        fi

