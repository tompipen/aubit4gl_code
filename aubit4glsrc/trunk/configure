			{ echo "$as_me:$LINENO: WARNING: ** Directory specified as prefix:" >&5
echo "$as_me: WARNING: ** Directory specified as prefix:" >&2;}
			{ echo "$as_me:$LINENO: WARNING: ** $prefix" >&5
echo "$as_me: WARNING: ** $prefix" >&2;}
