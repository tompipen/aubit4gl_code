echo "${ECHO_T}Detected:$A4GL_MENUTYPE_DETECTED" >&6

## ==================================================================
##            Decide on default EXTended REPorts driver
## ==================================================================

echo "$as_me:$LINENO: checking Default EXTended REPorts driver" >&5
echo $ECHO_N "checking Default EXTended REPorts driver... $ECHO_C" >&6
if test "$default_report" != ""; then
	#User can force EXREPORT driver default with --default-report=xxxx switch
	A4GL_PDFTYPE="$default_report"
else
	if test "$PDFBUILD" != "no"; then
		A4GL_PDFTYPE="PDF"
		A4GL_PDFTYPE_DETECTED="$A4GL_PDFTYPE_DETECTED $A4GL_PDFTYPE"
    fi
fi
echo "$as_me:$LINENO: result: $A4GL_PDFTYPE" >&5
echo "${ECHO_T}$A4GL_PDFTYPE" >&6
