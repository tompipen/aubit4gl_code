				#BISON="LANG=C LC_ALL=C; nice -n -20 $BISON -y"
				BISON="LANG=C LC_ALL=C $BISON -y"
			fi
		else
			BISON=no
			WARN_YACC=1
			{ $as_echo "$as_me:$LINENO: WARNING: cannot find bison in $BISON_PATH" >&5
$as_echo "$as_me: WARNING: cannot find bison in $BISON_PATH" >&2;}
		fi
