## ==================================================================
##            check what the byte ordering is:
## ==================================================================

    gcc order.c -o order

    ORDER=`./order`

	case "$ORDER" in
		10) #least byte first

cat >>confdefs.h <<\EOF
#define HAVE_LEAST_BYTE_FIRST 1
EOF

            echo "Byte order is 10"
        ;;

        01) #If it gives 01 then ntohs would be
			#define ntohs(x) (x)

cat >>confdefs.h <<\EOF
#define HAVE_LEAST_BYTE_FIRST 0
EOF

            echo "Byte order is 01"
	        ;;

        *)
            echo "Invalid byte order result: >>$ORDER<<"
            exit 5
	        ;;
    esac

